{
    "name": "jolicode/secret-santa",
    "description": "Throw a Secret Santa with your team!",
    "license": "MIT",
    "authors": [
        {
            "name": "JoliCode",
            "email": "coucou@jolicode.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "JoliCode\\SecretSanta\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JoliCode\\SecretSanta\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "adam-paterson/oauth2-slack": "^1.1.3",
        "bugsnag/bugsnag-symfony": "dev-symfony-5",
        "iflorespaz/oauth2-zoom": "dev-master",
        "jolicode/slack-php-api": "^2.0",
        "nelmio/security-bundle": "dev-symfony5-simple",
        "nyholm/psr7": "^1.2",
        "predis/predis": "^1.1",
        "restcord/restcord": "^0.3",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/asset": "^5.0",
        "symfony/console": "^5.0",
        "symfony/flex": "^1.4",
        "symfony/framework-bundle": "^5.0",
        "symfony/http-client": "^5.0",
        "symfony/monolog-bundle": "^3.3",
        "symfony/twig-bundle": "^5.0",
        "symfony/yaml": "5.0 as 4.4",
        "twig/twig": "^2.12",
        "wohali/oauth2-discord-new": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "symfony/browser-kit": "^5.0",
        "symfony/css-selector": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/stopwatch": "^5.0",
        "symfony/web-profiler-bundle": "^5.0",
        "symfony/web-server-bundle": "~4.4"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/pyrech/bugsnag-symfony"
        },
        {
            "type": "vcs",
            "url": "https://github.com/damienalexandre/NelmioSecurityBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/jiminny/oauth2-zoom"
        }
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BJXX8ZDDPMDF82T76S0FA56S",
            "allow-contrib": false
        }
    }
}
